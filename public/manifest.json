{
  "name": "MuslimSoulmate.ai",
  "short_name": "MySoulmate",
  "description": "Mindful matchmaking for Muslims",
  "start_url": "/",
  "display": "standalone",
  "background_color": "#0A3A2E",
  "theme_color": "#0A3A2E",
  "orientation": "portrait-primary",
  "icons": [
    {
      "src": "/mysouldna-logo.jpg",
      "sizes": "192x192",
      "type": "image/jpeg"
    },
    {
      "src": "/mysouldna-logo.jpg",
      "sizes": "512x512",
      "type": "image/jpeg"
    }
  ],
  "share_target": {
    "action": "/share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "media",
          "accept": ["image/*", "video/*"]
        }
      ]
    }
  }
}
